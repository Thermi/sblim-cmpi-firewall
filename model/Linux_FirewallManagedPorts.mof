[Description("This class defines the ports that are chosen to be Managed by the client of WBEM-SMTFIREWALL task")
]

class Linux_FirewallManagedPorts : CIM_SettingData { 

	[Description("Name of the Protocol the said port supports")]
	string Protocol;
 
	[Description("The starting Port-in case a range of ports are defined, in case of individual port-the port number")]
	string StartPort; 

	[Description("The End Port number-in case a range of ports are defined, else NULL")]
	string EndPort; 

	}; 
